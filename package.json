{
  "name": "vue-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "devbuild": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "better-scroll": "^1.13.2",
    "js-md5": "^0.7.3",
    "lib-flexible": "^0.3.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuescroll": "^4.9.0-beta.4",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4",
    "weixin-js-sdk": "^1.3.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^3.0.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.0",
    "postcss-pxtorem": "^4.0.1",
    "sass-loader": "^7.0.1",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-pxtorem": {
        "rootValue": 75,
        "propList": [
          "*"
        ],
        "selectorBlackList": [
          "fs"
        ],
        "replace": true,
        "mediaQuery": false,
        "unitPrecision": 3,
        "minPixelValue": 2
      }
    }
  }
}
